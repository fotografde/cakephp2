{
	"name": "cakephp/cakephp",
	"description": "Inofficial Fork of CakePHP",
	"type": "library",
	"keywords": ["framework"],
	"homepage": "https://github.com/fotografde/cakephp2",
	"license": "MIT",
	"authors": [
		{
			"name": "CakePHP Community",
			"homepage": "https://github.com/cakephp/cakephp/graphs/contributors"
		}
	],
	"require": {
		"php": "^8.0",
	  	"phpseclib/mcrypt_compat": "^1.0"
	},
	"suggest": {
		"ext-openssl": "You need to install ext-openssl or ext-mcrypt to use AES-256 encryption",
		"ext-mcrypt": "You need to install ext-openssl or ext-mcrypt to use AES-256 encryption"
	},
	"require-dev": {
	  "phpunit/phpunit": "^9",
	  "rector/rector": "*"
	},
	"config": {
		"vendor-dir": "vendor/",
		"process-timeout": 0
	},
	"autoload": {
		"exclude-from-classmap": [" /lib/Cake/Console/Templates/skel/", "/app/"]
	}
}
